@{ 
    @model ASP_Reg_Form.Models.User
}

<div class="container">
    <div class="row">
        <div class="col-md-2">Full Name:</div><div class="col-md-10">@Html.DisplayFor(model => model.FullName)</div>
    </div>
    <div class="row">
        <div class="col-md-2">First Name:</div><div class="col-md-10">@Html.DisplayFor(model => model.FirstName)</div>
    </div>
    <div class="row">
        <div class="col-md-2">Last Name:</div><div class="col-md-10">@Html.DisplayFor(model => model.LastName)</div>
    </div>
    <div class="row">
        <div class="col-md-2">Email:</div><div class="col-md-10">@Html.DisplayFor(model => model.Email)</div>
    </div>
    <div class="row">
        <div class="col-md-2">User Name:</div><div class="col-md-10">@Html.DisplayFor(model => model.UserName)</div>
    </div>
    <div class="row">
        <div class="col-md-2">Password:</div><div class="col-md-10">@Html.DisplayFor(model => model.Password)</div>
    </div>
    <div class="row">
        <div class="col-md-2">Phone:</div><div class="col-md-10">@Html.DisplayFor(model => model.Phone)</div>
    </div>
</div>
